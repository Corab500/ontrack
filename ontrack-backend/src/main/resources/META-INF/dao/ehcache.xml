<ehcache
        updateCheck="false">
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="300"
            timeToLiveSeconds="300"
            overflowToDisk="false"
            statistics="true"
            memoryStoreEvictionPolicy="LRU"/>
    <cache name="CONFIGURATION"
           maxElementsInMemory="50"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           statistics="true"
           overflowToDisk="false" />
    <cache name="PROJECT"
           maxElementsInMemory="10"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           statistics="true"
           overflowToDisk="false" />
    <cache name="BRANCH"
           maxElementsInMemory="50"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           statistics="true"
           overflowToDisk="false" />
    <cache name="VALIDATION_STAMP"
           maxElementsInMemory="100"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           statistics="true"
           overflowToDisk="false" />
    <cache name="BUILD"
           maxElementsInMemory="200"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           statistics="true"
           overflowToDisk="false" />
</ehcache>